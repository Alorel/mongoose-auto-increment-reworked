{
  "name": "mongoose-auto-increment-reworked",
  "version": "1.0.0",
  "description": "An auto-incrementing field generator for Mongoose 4 & 5",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf ./*.js ./*.js.map ./*.d.ts coverage",
    "reinstall": "rimraf node_modules package-lock.json && npm install",
    "tslint": "tslint -p ./tsconfig.json -s node_modules/custom-tslint-formatters/formatters -t grouped index.ts",
    "tslint:fix": "npm run tslint -- --fix",
    "test:debug": "cross-env DEBUG=mongoose-auto-increment-reworked npm test",
    "test": "nyc mocha --exit --full-trace -r ts-node/register -r source-map-support/register test.ts"
  },
  "dependencies": {
    "@types/debug": "*",
    "@types/mongoose": "*",
    "debug": "^3.0.0",
    "typescript-lazy-get-decorator": "^1.0.0"
  },
  "peerDependencies": {
    "mongoose": "^4.0.0 || ^5.0.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.1",
    "@types/lodash": "^4.14.93",
    "@types/mocha": "^2.2.46",
    "@types/uuid": "^3.4.3",
    "chai": "^4.1.2",
    "coveralls": "^3.0.0",
    "cross-env": "^5.1.3",
    "custom-tslint-formatters": "^2.1.1",
    "lodash": "^4.17.4",
    "mocha": "^5.0.0",
    "mongoose": "^4.0.0 || ^5.0.0",
    "nyc": "^11.0.0",
    "rimraf": "^2.6.2",
    "source-map-support": "^0.5.2",
    "ts-node": "^4.0.0",
    "tslint": "^5.9.0",
    "typescript": "^2.6.0",
    "uuid": "^3.2.1"
  },
  "keywords": [],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Alorel/mongoose-auto-increment-reworked.git"
  },
  "homepage": "https://github.com/Alorel/mongoose-auto-increment-reworked",
  "bugs": {
    "url": "https://github.com/Alorel/mongoose-auto-increment-reworked.git"
  },
  "author": {
    "name": "Arturas Molcanovas",
    "email": "a.molcanovas@gmail.com",
    "url": "https://github.com/Alorel"
  },
  "license": "MIT"
}
